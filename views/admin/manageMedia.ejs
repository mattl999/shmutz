<%- include ('../partials/start.ejs') %>
<button class="logout"></button>
<h1>Admin Page</h1> 
<%- include ('../partials/navbar.ejs') %> 
<div class="container-fluid mt-4 row">
    <div class="col-12 col-md-6">
        <form class="createForm" action="/admin/createSong" method="post">
            <h2>Add new Song:</h2>
            <label for="name">Song Title:</label>
            <input type="text" name="name" value="epic song">
            <label for="artist">Artist:</label>
            <input type="text" name="artist" value="artist">

            <label for="spotifyUrl">Spotify Song Link:</label>
            <p class="example mb-0"><strong> ex.</strong> https://open.spotify.com/track/3Vby4nGmtbDo7HDJamOWkT?si=df183b8d909749f5</p>
            <input type="text" name="spotifyUrl" value="random Url" >
            
            <label for="mp3File">mp3 File:</label>
            <input type="file" name="mp3File" id="" >
            <button type="submit" class="mt-3">Add Song</button>
        </form>
    </div>
    <div class="col-12 col-md-6">
        <h2>Add new Video:</h2>
    </div>
    <!-- <button onclick="testCors()">Test</button> -->
</div>


<%- include ('../partials/end.ejs') %> 